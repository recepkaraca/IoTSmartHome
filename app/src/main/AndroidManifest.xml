<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iot.iotgoogleassistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.NoActionBar">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".RelayControlActivity">
        </activity>
        <activity android:name=".AirConditionControlActivity">
        </activity>
        <activity android:name=".PirControlActivity">
        </activity>
        <activity android:name=".AssistantActivity">
        </activity>
        <activity android:name=".LCDControlActivity">
        </activity>
        <activity android:name=".AccountControlActivity">
        </activity>
        <activity android:name=".RegisterActivity">
        </activity>
        <activity android:name=".AddNFCActivity">
        </activity>
    </application>

</manifest>
